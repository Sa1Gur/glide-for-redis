{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C# benchmark Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/benchmarks/csharp/bin/Debug/net8.0/csharp_benchmark.dll",
            "args": [
                "--dataSize",
                "1024",
                "--resultsFile",
                "test.json",
                "--concurrentTasks",
                "4",
                "--clients",
                "all",
                "--host",
                "localhost",
                "--clientCount",
                "4"
            ],
            "cwd": "${workspaceFolder}/benchmarks/csharp",
            "console": "internalConsole",
            "stopAtEntry": true
        }
    ]
}
